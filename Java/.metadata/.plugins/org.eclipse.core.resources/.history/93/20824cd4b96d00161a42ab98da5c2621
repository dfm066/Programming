	}
	protected void MarathiDemo(){
		String name;
		name =  sc.nextLine();
		Character.UnicodeScript myscript = Character.UnicodeScript.forName(name);
		char baseChar = '\u0904';
		for(int i = 1; i <= noOfChar; i++){
			
			if(i%5==0)
